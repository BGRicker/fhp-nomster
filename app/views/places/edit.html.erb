<div class="cool-box col-xs-10 col-xs-offset-1">
  <h1><%= "Edit #{@place.name}"%></h1>
  <br/>
  <%= simple_form_for @place do |f| %>
    <%= f.input :name %>
    <%= f.input :address %>
    <%= f.input :description %>
    <br/>
    <%= f.submit 'Update', class: 'btn btn-success' %>
  <% end %>
</div>
